{
    "compilerOptions": {
        "target": "es2017",
        "lib": [
            "dom",
            "dom.iterable",
            "esnext"
        ],
        "allowJs": true,
        "skipLibCheck": true,
        "strict": true,
        "noEmit": true,
        "esModuleInterop": true,
        "module": "esnext",
        "moduleResolution": "bundler",
        "resolveJsonModule": true,
        "isolatedModules": true,
        "jsx": "preserve",
        "incremental": true,
        "baseUrl": "./",
        "plugins": [
            {
                "name": "next"
            }
        ],
        "paths": {
            "@/*": [
                "./*"
            ]
        },
        "allowSyntheticDefaultImports": true,
        "resolvePackageJsonExports": true,
        "resolvePackageJsonImports": true,
        "preserveConstEnums": true,
        "noImplicitAny": true,
        "noImplicitThis": true,
        "strictNullChecks": true,
        "strictFunctionTypes": true,
        "strictBindCallApply": true,
        "strictPropertyInitialization": true,
        "strictBuiltinIteratorReturn": true,
        "alwaysStrict": true,
        "useUnknownInCatchVariables": true
    },
    "files": [
        "./next-env.d.ts",
        "./.next/types/cache-life.d.ts",
        "./.next/types/app/api/scriptLogs/route.ts",
        "./.next/types/app/api/solicitations/route.ts",
        "./.next/types/app/api/solicitations/[id]/route.ts",
        "./.next/types/app/api/solicitations/[id]/comments/route.ts",
        "./.next/types/app/api/solicitations/[id]/logs/route.ts",
        "./.next/types/app/api/solicitations/counts/route.ts",
        "./.next/types/app/api/solicitations/counts/summary/route.ts",
        "./.next/types/app/api/solicitations/logs/route.ts",
        "./.next/types/app/api/solicitations/search/route.ts",
        "./.next/types/app/api/stats/route.ts",
        "./.next/types/app/api/stats/[id]/route.ts",
        "./.next/types/app/api/users/[id]/route.ts",
        "./.next/types/app/contacts/page.ts",
        "./.next/types/app/login/page.ts",
        "./.next/types/app/logs/page.ts",
        "./.next/types/app/register/page.ts",
        "./.next/types/app/reset-password/page.ts",
        "./.next/types/app/settings/page.ts",
        "./.next/types/app/solicitations/page.ts",
        "./.next/types/app/solicitations/[id]/page.ts",
        "./.next/types/app/solicitations/[id]/jsonEdit/page.ts",
        "./.next/types/app/stats/page.ts"
    ],
    "include": [
        "next-env.d.ts",
        "app/**/*.{ts,tsx}",
        "components/**/*.{ts,tsx}",
        "lib/**/*.{ts,tsx}",
        "hooks/**/*.{ts,tsx}",
        "config/**/*.{ts,tsx}",
        "scripts/**/*.{ts,tsx}",
        ".next/types/**/*.ts"
    ],
    "exclude": [
        "node_modules",
        "functions"
    ]
}
